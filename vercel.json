{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/*.js": {
            "includeFiles": "drizzle/"
        }
    }
}