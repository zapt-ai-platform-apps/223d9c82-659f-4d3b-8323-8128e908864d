{
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "^/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "^/(?!api).*",
            "dest": "/index.html"
        }
    ],
    "functions": {
        "api/*.js": {
            "includeFiles": "../drizzle/**"
        }
    }
}